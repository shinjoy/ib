/*  popup  */
.popuplayerbg { position: absolute; top: 0; bottom: 0; left: 0; right: 0; width: 230px; height: 348px; background: #000; opacity: 0.7; display: none; z-index: 1000; }
.popupwrapper {  position: absolute; left: 0; top: 0; bottom: 0; right: 0; width: 230px; height: 348px; z-index: 1001; }
.popupwrapper580 { width: 230px; margin: 200px auto 0 auto; -webkit-box-shadow: 0 0 12px #000; -webkit-border-radius: 10px; }

/* 아래서 나오는 슬라이드 */
.bottomBox{
	position:absolute;
	bottom:-100%;
	left:0%;
	height:100%;
	background-color: white;
	width: 100%;
	display: block;
	z-index:9999;
}

.disPlayNone{display:none!important;}



.bottomBox-inner {
   /*  position: absolute; */
    width:100%;
    top: 0px;
    bottom: 0px;
   /*  overflow: scroll; */
    -webkit-overflow-scrolling: touch;
}

/* 왼쪽에서 오른쪽 슬라이드 */
.rightBox{
	position:absolute;
	bottom:0%;
	top:0;
	right:-100%;
	height:100%;
	background-color: white;
	width: 100%;
	display: block;
	z-index:999999999;
}
.rightBox-inner {
/*     position: absolute; */
    width:100%;
    top: 0px;
    bottom: 0px;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
}

