.pop_tb_basic thead tr:first-child { background:#d8dce3; border-top:#b7bfcd solid 1px; }
.pop_tb_basic thead th { font-weight:bold; color:#233357; padding:0.5em 0.5em; font-size:0.938rem; border-right:#b7bfcd solid 1px; border-bottom:#b7bfcd solid 1px; text-align:center; }
.pop_tb_basic thead th:first-child { border-left:#b9c1ce solid 1px; }

.pop_tb_basic tbody th { border-top:#dedfdf solid 1px; border-left:#cfcfd0 solid 1px; border-right:#cfcfd0 solid 1px; background:#f6f6f6; font-weight:normal; font-size:0.875rem; padding:0.313rem 0rem; text-align:center; }
.pop_tb_basic tbody td { border-top:#dedfdf solid 1px; border-right:#cfcfd0 solid 1px; color:#6d6c74; text-align:center; color:#6d6c74; font-size:0.875rem; padding:0.313rem 0.5rem; }
.pop_tb_basic tbody tr td:first-child { border-left:#cfcfd0 solid 1px; }
.pop_tb_basic tbody tr:first-child td { border-top:none; }
.pop_tb_basic tbody tr:hover { background:#f5f7f8; /*f7f8f9*/ }
.pop_tb_basic tbody tr.tr_selected { background:#edf1f2; /*f7f8f9*/ }

.pop_tb_basic .icon_carNum { background:#eceef0; line-height:1; font-size:0.75rem; padding:0.188rem 0.313rem; text-align:center; min-width:1.5rem; display:inline-block; font-weight:bold; color:#555; border:#9499a1 solid 1px; border-radius:0.188rem; }
.pop_tb_basic .usetime { color:#555; font-size:0.875rem; letter-spacing:0em; }
.pop_tb_basic .car_user { color:#202020; font-weight:600; }
.pop_tb_basic .txt_left { text-align:left; }
.pop_tb_basic .reviewtext { clear: both; padding-top: 0.375rem; text-align: left; font-size: 0.750rem;}


/* 모달 팝업 분석자 추가  */
.pop_tb_basic tbody td .del{background: #fff;padding: 0.069rem 0.375rem 0.069rem;font-size: 0.75rem;border: #a5a5a5 solid 1px;background: #fefefe;border-radius: 0.188rem;color: #333;}
.pop_tb_basic tbody td .plus {background: #4267b2;padding: 0.125rem 0.375rem 0.125rem;font-size: 0.75rem;border-radius: 0.188rem;color: #fff;}
.pop_tb_basic tbody td.plustab {background: #e9ecf0;padding: 0.688rem 0;}

.pool_seachZone { text-align:center; margin-bottom:1em; }
.pool_seachZone input { border:#7d8fa2 0.25em solid; border-radius:0; font-size:1rem; box-sizing:border-box; padding:0.1em 0.3em; width:60%; }
.pool_seachZone button { background:url(../images_m/work/bg_btn_pool.png) no-repeat center center; color:#fff; font-size:1rem; display:inline-block; margin-left:1em; padding:0.32em 1em; }


.pop_tb_basic2 { letter-spacing:-0.06em; width:100%; border-bottom:#bec2c7 solid 1px; table-layout: fixed; }
.pop_tb_basic2 caption { display:none;}
.pop_tb_basic2 thead tr { background:#d8dce3; }
.pop_tb_basic2 thead tr:first-child { background:#d8dce3; border-top:#b7bfcd solid 1px; }
.pop_tb_basic2 thead th { font-weight:bold; color:#233357; padding:0.5rem 0.313rem; font-size:0.813rem; border-right:#b7bfcd solid 1px; border-bottom:#b7bfcd solid 1px; text-align:center; }
.pop_tb_basic2 thead th:first-child { border-left:#b9c1ce solid 1px; }

.pop_tb_basic2 tbody td { border-top:#dedfdf solid 1px; border-right:#cfcfd0 solid 1px; color:#6d6c74; text-align:center; color:#6d6c74; font-size:0.75rem; padding:0.313rem 0.313rem; }
.pop_tb_basic2 tbody tr td:first-child { border-left:#cfcfd0 solid 1px; }
.pop_tb_basic2 tbody tr:first-child td { border-top:none; }
.pop_tb_basic2 .txt_right { text-align:right; }

/*게시판 페이지 btn*/
.btnPageZone { margin-top:1rem; text-align:center; font-family:Verdana, Geneva, sans-serif; vertical-align:middle; }
.btnPageZone button { display:inline-block; vertical-align:middle; }
.btnPageZone button span {  font-size:1rem; vertical-align:middle; height:1.6rem; width:1.2rem; overflow:hidden; display:inline-block;  }
.btnPageZone button em { visibility:hidden; }
.btnPageZone button.pre_end_btn { background:url(../images_m/common/btn_page_prevend.png) no-repeat center center; background-size:60%; width:1.6rem; }
.btnPageZone button.pre_btn { background:url(../images_m/common/btn_page_prev.png) no-repeat center center; background-size:48%; margin-right:0.5em; }
.btnPageZone button.next_btn { background:url(../images_m/common/btn_page_next.png) no-repeat center center; background-size:48%; margin-left:0.5em; }
.btnPageZone button.next_end_btn { background:url(../images_m/common/btn_page_nextend.png) no-repeat center center; background-size:60%; width:1.6rem; }


/*페이징버튼*/
.btnPageZone span { }
.btnPageZone span a { border-left:#dadada solid 1px; line-height:1.6rem; font-size:1rem; padding:0em 0.5em; text-align:center; vertical-align:middle; display:inline-block; }
.btnPageZone .pre_btn + span a { border-left:none; }
.btnPageZone span.current a { border:#5d697c solid 1px; background:none; color:#242424; font-weight:bold; padding:0em 0.6em; }
.btnPageZone span.current + span a { background:none; border-left:none; }

.btn_baordZone { border-top:#7f7f7f solid 1px; margin-top:20px; padding-top:20px; text-align:center; }
.btn_baordZone a { border-radius:2px; letter-spacing:-0.8px; color:#fff; padding:6px 15px 7px; line-height:16px; height:16px; font-size:12px; vertical-align:middle; margin-left:10px; display:inline-block; min-width:60px;  }
.btn_baordZone a:first-child { margin-left:0px; }
.btn_baordZone .btn_blueblack { background:#59647a; }
.btn_baordZone .btn_witheline { border:#bdc3d1 solid 1px; color:#232c3f; }


/* 친밀도표시 */
.relation_mark em { width:0.9em; height:0.9em; border:1px solid #e7b734; margin-left:2px; cursor:pointer; background:#ffd359;  display:inline-block; }
.relation_mark, .relation_mark em, .relation_mark span { vertical-align:middle; }

.wrap_b_box { float:none; *zoom:1; width:100%; }
.wrap_b_box:after { content:""; display:block; height:0; clear:both; visibility:hidden; }
.wrap_b_box .fl_w2 { float:left; width:4.5em; padding:0.3em 0; font-size:0.8rem; }
.wrap_b_box .fl_w5 { float:left; width:4.5em; padding:0.3em 0; font-size:1.2rem; }
.wrap_b_box .fl_w6 { float:left; width:5.5em; padding:0.3em 0; font-size:1.2rem; }
.wrap_b_box .fr_auto { width:auto; overflow:hidden; padding-bottom:1px; }

.p_notice { color:#737373; line-height:1.2; font-size:0.813rem; }
.p_notice strong { color:#555; }
.redpoint { color:#ff4a4a; }
.bluepoint { color:#3685d9; }
.redstar { color:#ff4a4a; padding:0 0.3em; vertical-align:middle; }

