
/* skyblue */
.btn_blue {
    background: linear-gradient(to bottom, #79BBFF 5%, #378DE5 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#79BBFF', endColorstr='#378DE5',GradientType=0);
    background-color: #79BBFF;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-radius: 4px;
    display: inline-block;
    cursor: pointer;
    color: #FFFFFF!important;
    font-family: 맑은 고딕;
    font-size: 8pt;
    font-weight: bold;
    padding: 4px 8px;
    text-decoration: none;
   }
.btn_blue:hover {
  
   text-shadow: #1e4158 0 1px 0;
   background: #006bb3;
   background: -webkit-gradient(linear, left top, left bottom, from(#0592f0), to(#006bb3));
   background: -webkit-linear-gradient(top, #0592f0, #006bb3);
   background: -moz-linear-gradient(top, #0592f0, #006bb3);
   background: -ms-linear-gradient(top, #0592f0, #006bb3);
   background: -o-linear-gradient(top, #0592f0, #006bb3);
   background-image: -ms-linear-gradient(top, #0592f0 0%, #006bb3 100%);
   color: #edff00!important;
   }

