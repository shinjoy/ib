/* contents */
#content_detail {padding:38px 0; max-width:90%; margin:0 auto; }
.greetings {background:url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/bg_greeting.jpg) no-repeat right bottom; background-size:90%; padding-bottom:180px; max-width:70%; margin:0 auto;  }
.txt_dark_big {font-size:1.5em; color:#010101; }
.vision { min-height:452px; background:url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/img_vision.gif) no-repeat 0 0;}

/* list_style */
.lstyle_plus,
.list_style,
.lstyle_arrow,
.lstyle_circle,
.lstyle_dot	 {
	clear: both;
	}
.lstyle_plus li:first-child,
.list_style li:first-child,
.lstyle_arrow li:first-child,
.lstyle_circle li:first-child,
.lstyle_dot li:first-child	{
	margin-top: 0;
	}
.list_style {
	margin-left: 6px;
	}
.lstyle_plus > li	{
	margin-top: 20px;
	padding-left: 16px;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/depth3_arrow.gif) no-repeat 0 2px;
	color: #1d78d5;
	}
.lstyle_plus > li > ul,
.lstyle_plus > li > p	{
	margin: 10px 0 0 -10px;
	color: #636363;
	}
.lstyle_plus > li > ul > li,
.lstyle_plus > li > p,
.list_style > li,
p.list_style {
	margin-top: 4px;
	padding-left: 12px;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/list_arrow.gif) no-repeat 0 7px;
	}
.lstyle_plus > li > p.bgNone_txt,
.list_style > li p.bgNone_txt,
.list_style > li > ul.bgNone_txt li	{
	margin-left: -15px;
	padding-left: 0;
	background: none;
	}
.list_style > li p.bgNone_txt {
	margin-left: 10px;
	}

.lstyle_arrow {
	color: #767676;
	}
.lstyle_plus > li > ul > li > ul > li,
.lstyle_plus > li > ul > li > p,
.list_style > li > ul > li,
.list_style > li > p,
.lstyle_arrow > li,
p.lstyle_arrow	{
	margin-top: 3px;
	padding-left: 10px;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/arrow.gif) no-repeat 0 6px;
	}
.lstyle_plus > li > ul > li > ul > li > ul > li,
.lstyle_plus > li > ul > li > ul > li > p,
.list_style > li > ul > li > ul > li,
.list_style > li > ul > li > p,
.lstyle_arrow > li > ul > li,
.lstyle_circle > li,
p.lstyle_circle	 {
	margin-top: 3px;
	padding-left: 10px;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/gray_arrow.gif) no-repeat 0 7px;
	}
p.lstyle_circle {
	margin-bottom: 10px;
	}

.lstyle_plus > li > ul > li > ul > li > ul > li > ul > li,
.lstyle_plus > li > ul > li > ul > li > ul > li p,
.list_style > li > ul > li > ul > li > ul > li,
.list_style > li > ul > li > ul > li > p,
.lstyle_dot > li,
.icon_lstyle > li,
p.lstyle_dot	{
	margin-top: 3px;
	padding-left: 9px;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/dot_arrow.gif) no-repeat 0 7px;
	}
.icon_lstyle > li {
	margin: 0;
	}
table td.txt_dot,
span.txt_dot	 {
	padding-left: 24px;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/dot_arrow.gif) no-repeat 15px 15px;
	}
span.txt_dot {
	display: block;
	padding-left: 9px;
	background-position: 0 7px;
	}
.lstyle_plus > li > ul > li > ul > li > ul > li > ul > li > ul > li,
.lstyle_plus > li > ul > li > ul > li > ul > li > ul > li p,
.list_style > li > ul > li > ul > li > ul > li > ul > li,
.list_style > li > ul > li > ul > li > ul > li > p,

.lstyle_dot > li > ul > li,
.lstyle_dot > li > p {
	padding-left: 8px;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/black_arrow.gif) no-repeat 0 8px;
	}

.list_style li table {
	margin: 0 0 15px -11px;
	}
.list_style li table th {
	padding: 7px 5px;
	}
.list_style li table td {
	padding: 10px 5px;
	}
table ul.lstyle_dot {
	margin-left: 0;
	}
table ul.lstyle_dot > li:first-child {
	margin-top: 0;
	}

/* 주석 */
.comment,
.span_comment	 {
	display: inline-block;
	width: 700px;
	margin-top: 20px;
	padding: 20px 0;
	border-top: dotted 1px #acacac;
	border-bottom: dotted 1px #acacac;
	color: #767676;
	font-size: 0.95em;
	}
.span_comment {/* 목록 안에 있는 주석 */
	margin-top: 0;
	padding: 0;
	border: none;
	}
.comment li {
	margin-top: 3px;
	}
.comment li:first-child {
	margin-top: 0;
	}
.comment li span.num {
	display: inline-block;
	width: 13px;
	color: #ff6600;
	vertical-align: top;
	}
.comment li span.txt {
	display: inline-block;
	width: 680px;
	margin-right: 3px;
	}
.comment_num {
	display: inline-block;
	margin: 0 2px;
	color: #ff6600;
	}
.summary_process01 {
	height: 277px;
	margin: 0 auto 10px;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/summary_process01.gif) no-repeat 0 0;
	text-indent: -9999px;
	}
.summary_process02 {
	height: 225px;
	margin: 0 auto 20px;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/summary_process02.gif) no-repeat 0 0;
	text-indent: -9999px;
	}
.summary_process03 {
	height: 200px;
	margin: 0 auto;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/summary_process03.gif) no-repeat 0 0;
	text-indent: -9999px;
	}

/* 오시는길 */
.map01 {
	margin-bottom: 10px;
	}
.way {
	display: inline-block;
	max-width: 605px;
	min-height: 70px;
	margin: 33px 0;
	padding-left: 95px;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/location_bg.gif) no-repeat 0 0;
	}
.way_list {
	float: left;
	}
.way_list li {
	padding-left: 11px;
	background: url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/contents/gray_arrow.gif) no-repeat 0 5px;
	}
.way_list li:first-child {
	margin-bottom: 12px;
	}
.way_list li .color_black {
	color: #252525;
	font-weight: bold;
	}
.way_list li .point01 {
	font-weight: bold;
	}
.way_list li.etc {
	background: none;
	color: #707070;
	font-size: 0.95em;
	letter-spacing: -0.1em;
	}
.way_list li.etc span {
	font-weight: bold;
	}

.way_btn {
	float: right;
	margin-top: 35px;
	}
.btn_area_center {
	text-align:center;
	margin-top:12px;
	}
.graph {height:230px; }
.graph dt {margin-bottom:5px; color:#797979; font-size:0.95em;  text-align:center; line-height:1.4em;  }
.graph dt strong {color:#2c5089; font-size:14px;  font-weight:bold;  }

.desc_graph {font-size:0.95em; line-height:1.3em; overflow:hidden; }
.desc_graph li {clear:both; height:38px; margin-bottom:18px;}
.desc_graph li span {float:left; width:60px; text-align:center; }
.desc_graph li p {float:left; width:145px; padding:4px 0; }

.civil_point01 {
	margin-bottom: 25px;
	color: #010101;
	letter-spacing: -0.1em;
	line-height: 1.2;
	font-size: 2.2em;
	font-weight: bold;
	font-family: "돋움", Dotum;
	}
.civil_point02,
.civil_point03	 {
	margin-bottom: 37px;
	color: #11479c;
	letter-spacing: -0.1em;
	line-height: 1.3;
	font-size: 1.5em;
	font-weight: bold;
	}
.civil_point03 {
	color: #636363;
	font-weight: normal;
	}


@media all and (max-width: 600px)  {
	#content_detail {padding:25px 0; max-width:88%; margin:0 auto; }
	h1.stitle {padding: 13px 12px 12px; color:#fff; font-size:1.1em;}
	.location span {padding:10px; }
	.location .ico_home {width:16px;  background:url(file:///D|/%5B01%5D%EC%97%85%EB%AC%B4/%5B02%5D%20%EC%BD%94%EB%94%A9/%5B%EC%9B%90%EB%B3%B8%5D/%5B12%5D%20PASS%20%EB%AA%A8%EB%B0%94%EC%9D%BC/images/ico_group.png) no-repeat 11px -48px; background-size:35px;  padding-top:14px;}
	.greetings {background-size: 100%; padding-bottom:120px; max-width:88%; }
	.vision {height:260px; background-size: 98%;}
	.summary_process01 {height: 200px; background-size: 105%;}
	.summary_process02 {height: 170px; background-size: 105%;}
	.summary_process03 {height: 150px; background-size: 105%;	}
	.img_auto img {width: 100%;}
	.btn_area_center a {display: block; margin-bottom: 5px;}
	.civil_point01	{margin-bottom: 13px; font-size: 1.3em;}
	.civil_point02, .civil_point03 {margin-bottom: 20px; font-size: 1em; letter-spacing: 0;}

}




/*추가20170714 메모설정 */
/*메모  아이콘*/
.memo_list_m li {width:100%;float:none; *zoom:1; font-size: 0.938rem;}
.memo_list_m li .memo_icon { display:inline-block; width:1rem; height: 1.1rem;   }


/*메모설정 _ 수신화면*/
/*수신확인 테이블*/
.tb_list_basic3 { letter-spacing:-0.08px; width:100%; border-top:#b9c1ce solid 1px; border-bottom:#9298a2 solid 1px; }
.tb_list_basic3 thead tr { background:#d8dce3; border-right:#b9c1ce solid 1px; }
.tb_list_basic3 thead tr:first-child { background:#d8dce3; }
.tb_list_basic3 thead th { color:#3b4354; line-height:1; font-size:0.75rem; padding:0.6rem 0.6rem; border-left:#b9c1ce solid 1px; border-bottom:#b9c1ce solid 1px; }
.tb_list_basic3 thead th:first-child { border-left:#b9c1ce solid 1px; }

.tb_list_basic3 tbody tr { border-left:#cacbce solid 1px; border-right:#cacbce solid 1px; }
.tb_list_basic3 tbody th { font-weight:bold; background:#eff1f4; padding:0.6rem 0.6rem; border-top:#cacbce solid 1px; border-left:#cacbce solid 1px; line-height:1; font-size:0.875rem; }
.tb_list_basic3 tbody td { border-top:#e6e6e6 solid 1px; border-left:#e6e6e6 solid 1px; line-height:1; font-size:0.875rem; padding:0.6rem 0.6rem; color:#6d6c74; text-align:center; letter-spacing:-0.05em; color:#6d6c74; }
.tb_list_basic3 tbody tr td:first-child { border-left:#e6e6e6 solid 1px; }
.tb_list_basic3 tbody tr:first-child td { font-family:"돋움"; border-top:none; }
.tb_list_basic3 tbody tr.tr_selected { background:#f5f7f8; }

.tb_list_basic3 .state_icon_send { padding:0; }
.tb_list_basic3 .joinmember { text-align:left; }
.tb_list_basic3 .joinmember strong { font-weight:bold; color:#202020; }
.tb_list_basic3 .joinmember span { font-weight:normal; font-size:0.75rem; color:#202020; margin-left:0.2rem; }
.tb_list_basic3 .joinmember em { font-weight:normal; font-size:0.75rem; margin-left:0.5rem; color:#898989!important; }

/*업무메모아이콘*/
.state_icon_check { padding-left:1.375rem; font-size:0.938rem; font-weight:bold; color:#252525; display:inline-block; min-width:1.25rem; height:1.25rem; background:url(../images_m/work/icon_check.png) no-repeat 0 center; background-size:1.125rem 1.125rem; vertical-align:middle; }
.state_icon_order { padding-left:1.375rem; font-size:0.938rem; font-weight:bold; color:#252525; display:inline-block; min-width:1.25rem; height:1.25rem; background:url(../images_m/work/icon_order.png) no-repeat 0 center; background-size:1.125rem 1.125rem; vertical-align:middle; line-height:1.25rem; }
.state_icon_send { padding-left:1.375rem; font-size:0.938rem; font-weight:bold; color:#252525; display:inline-block; min-width:1.25rem; height:1.25rem; background:url(../images_m/work/icon_send.png) no-repeat 0 center; background-size:1.125rem 1.125rem; vertical-align:middle; }
.state_icon_memo { padding-left:1.375rem; font-size:0.938rem; font-weight:bold; color:#252525; display:inline-block; min-width:1.25rem; height:1.25rem; background:url(../images_m/work/icon_memo.png) no-repeat 0 center; background-size:1.125rem 1.125rem; vertical-align:middle; }
.state_icon_no_check { padding-left:1.375rem; font-size:0.938rem; font-weight:bold; color:#252525; display:inline-block; min-width:1.25rem; height:1.25rem; background:url(../images_m/work/icon_check_off.png) no-repeat 0 center; background-size:1.125rem 1.125rem; vertical-align:middle; }
.state_icon_check em, .state_icon_order em, .state_icon_send em, .state_icon_memo em, .state_icon_no_check em { vertical-align:middle; }


/*파일함 테이블*/
.file_searchbox { float:none; *zoom:1; padding:0.875rem 0.875rem 0.875rem; border-bottom:#d8d8d8 solid 1px; }
.file_searchbox:after { content:""; display:block; height:0; clear:both; visibility:hidden; }
.file_searchbox .fl_block { float:left; width:6rem; }
.file_searchbox .fr_block { width:auto; overflow:hidden; padding-left:0.6rem; }
.file_resultBox .title { padding:0.625rem 0.875rem; background:#f2f2f2; border-bottom:#cacaca dashed 1px; border-top:#d8d8d8 solid 1px; font-size:0.875rem; vertical-align:middle; line-height:1; color:#555; font-weight:bold; }
.file_resultBox .title:first-child { border-top:none; }
.file_resultBox .list_st_bluedot { padding:0.8rem 0.875rem; }
.file_resultBox .list_st_bluedot li { background:url(../images_m/work/bu_dot_file.png) no-repeat left 0.4rem; background-size:0.3rem; padding-left:0.7rem; padding-bottom:0.4rem; line-height:1; font-size:1rem; }
.file_resultBox .list_st_bluedot li a { color:#202020; font-weight:bold; }
.file_resultBox .list_st_bluedot li a em { font-weight:bold; }
.file_resultBox .list_st_bluedot .file_agent { font-size:0.875rem; margin-left:1rem; color:#202020; }
.file_resultBox .list_st_bluedot .time { font-size:0.875rem;  margin-left:0.3rem;  color:#898989; }
