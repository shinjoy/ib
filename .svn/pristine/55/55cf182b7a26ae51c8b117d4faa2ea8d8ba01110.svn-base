    color: #00aff0;
    font-weight: 700;
}

.welcome-text p { direction: ltr; text-align:right; word-break: keep-all; }


.welcome-text .point { font-size:1em; color:yellow; vertical-align:middle;  }
.welcome-text .point i { font-size:1.3em; vertical-align:middle; margin-right:2px; }

.home-button {
	position:relative;
