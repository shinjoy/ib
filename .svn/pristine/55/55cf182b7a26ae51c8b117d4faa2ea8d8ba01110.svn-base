}

.welcome-text .andver_des { position:absolute; width:100%; text-align:center; top:60px; left:0; display:none; font-size:1.25rem; }
.welcome-text .iosver_des { position:absolute; width:100%; text-align:center; top:60px; left:0; display:none; font-size:1.25rem; }
.welcome-text .ver_notice { margin-top:20px; color:#00aff0; display:block; }
.welcome-text .ver_notice a { color: #00aff0; text-decoration: underline; }

